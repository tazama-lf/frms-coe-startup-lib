{
  "name": "@tazama-lf/frms-coe-startup-lib",
  "version": "2.3.0-rc.2",
  "description": "FRMS Center of Excellence startup package library",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tazama-lf/frms-coe-startup-lib.git"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "@tazama-lf:registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "npx rimraf lib node_modules coverage package-lock.json",
    "dry": "npm publish --dry-run",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:eslint-unused": "eslint --report-unused-disable-directives \"**/*.ts\"",
    "lint:eslint-no-inline": "eslint --no-inline-config \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@tazama-lf/frms-coe-lib": "4.2.0-rc.3",
    "nats": "^2.28.2"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "dotenv": "^16.1.2",
    "eslint": "^8.57.0",
    "eslint-config-love": "^52.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
